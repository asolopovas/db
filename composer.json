{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "algolia/algoliasearch-client-php": "^4.11",
        "doctrine/dbal": "^3.3.7",
        "guzzlehttp/guzzle": "^7.9.2",
        "laravel/framework": "^11.0",
        "laravel/helpers": "^v1.5.0",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.0",
        "laravel/tinker": "^v2.7.2",
        "league/csv": "^9.1.0",
        "mikehaertl/phpwkhtmltopdf": "^2.5.0",
        "phpoffice/phpspreadsheet": "^1.24",
        "predis/predis": "^2.0",
        "respect/validation": "^2.2.3",
        "tymon/jwt-auth": "^2.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.3",
        "beyondcode/laravel-dump-server": "^2.0",
        "fakerphp/faker": "^1.19",
        "filp/whoops": "^2.14",
        "laravel/telescope": "^5.0",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^8.5",
        "phpunit/phpunit": "^11.0",
        "symfony/css-selector": "^6.1",
        "symfony/dom-crawler": "^6.1",
        "zurb/foundation": "^6.7"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper",
                "laravel/telescope"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
